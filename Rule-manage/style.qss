QPushButton{
background-color: qconicalgradient(cx:0.5, cy:0.522909, angle:179.9, stop:0.494318 rgba(214, 214, 214, 255), stop:0.5 rgba(236, 236, 236, 255));
border-style:outset;
border:2px;
border-radius: 8px;
padding:3px;
font: 10pt "Microsoft YaHei";
}
QPushButton:pressed{
background-color:rgba(100,255,100,200);
border-color:rgba(255,255,255,30);
border-style:inset;
color:rgba(0,0,0,100);

}
QPushButton:hover{
/*background-color: #f26522;*/
background-color: #4169E1;
color:#fff;
}
QMessageBox QPushButton{
background-color:rgb(255, 255, 255);
border-color:black;
}


QPushButton#outputfile:pressed,#addputton:pressed,#re_chose:pressed{
background-color:rgba(255,255,100,200);
border-color:rgba(255,255,100,30);
border-style:inset;
color:rgba(0,0,0,100);

}
QPushButton#outputfile:hover,#addputton:hover,#re_chose:hover{
background-color: #4169E1;
color:#fff;
}
