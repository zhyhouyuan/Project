QMenuBar {
background: rgb(187, 212, 238);
border: 1px solid rgb(111, 156, 207);
border-left: none;
border-right: none;
}
QMenuBar::item {
border: 1px solid transparent;
padding: 5px 10px 5px 10px;
background: transparent;
}
QMenuBar::item:enabled {
color: rgb(2, 65, 132);
}
QMenuBar::item:!enabled {
color: rgb(155, 155, 155);
}
QMenuBar::item:enabled:selected {
border-top-color: rgb(111, 156, 207);
border-bottom-color: rgb(111, 156, 207);
background: rgb(198, 224, 252);
}
QMenu {
border: 1px solid rgb(111, 156, 207);
background: rgb(232, 241, 250);
}
QMenu::item {
height: 22px;
padding: 0px 25px 0px 20px;
}
QMenu::item:enabled {
color: rgb(84, 84, 84);
}
QMenu::item:!enabled {
color: rgb(155, 155, 155);
}
QMenu::item:enabled:selected {
color: rgb(2, 65, 132);
background: rgba(255, 255, 255, 200);
}
QMenu::separator {
height: 1px;
background: rgb(111, 156, 207);
}
QMenu::indicator {
width: 13px;
height: 13px;
}
QMenu::icon {
padding-left: 2px;
padding-right: 2px;
}
QHeaderView{
border: none;
background: transparent;
min-height: 30px;
}
QHeaderView::section:horizontal {
border: 1px solid rgb(220, 220, 220);
color: rgb(2, 65, 132);
background: transparent;
padding-left: 5px;
}
QHeaderView::section:horizontal:hover {
color: white;
background: rgb(0, 78, 161);
}
QHeaderView::section:horizontal:pressed {
color: white;
background: rgb(6, 94, 187);
}

QScrollBar:horizontal {
height: 15px;
background: transparent;
margin-top: 0px;
margin-bottom: 0px;
}
QScrollBar::handle:horizontal {
height: 15px;
min-width: 30px;
background: rgb(200, 200, 200);
margin-left: 20px;
margin-right: 20px;
}
QScrollBar::handle:horizontal:hover {
background: rgb(220, 220, 220);
}


QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal {
background: transparent;
}

/**********-**********/
QScrollBar:vertical {
width: 15px;
background: transparent;
margin-left: 0px;
margin-right: 0px;
}
QScrollBar::handle:vertical {
width: 15px;
min-height: 30px;
background: rgb(200, 200, 200);
margin-top: 20px;
margin-bottom: 20px;
}
QScrollBar::handle:vertical:hover {
background: rgb(220, 220, 220);
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
background: transparent;
}

QScrollBar#verticalScrollBar:vertical {
margin-top: 30px;
}

QLineEdit {
border-radius: 8px;
height: 25px;
border: 1px solid rgb(111, 156, 207);
background: white;
}
QLineEdit:enabled {
color: rgb(84, 84, 84);
}
QLineEdit:enabled:hover, QLineEdit:enabled:focus {
color: rgb(51, 51, 51);
}
QLineEdit:!enabled {
color: rgb(80, 80, 80);
}

