QWidget#transparentWidget {
        background: transparent;
}
QTreeWidget{
    background: rgb(232, 241, 252);
    /* set the font properties*/
    font-size:16px;
    color: black;
}

QTreeWidget::item{
    margin:5px;
    background: rgb(232, 241, 252);
    background-clip: margin;
}

QTreeWidge::item:hover {
    background: rgb(69, 187, 217);
    color:rgb(255,255,255);
}

QTreeWidge::item:selected:active{
    background: rgb(63, 200, 168);
    color:rgb(255,255,255);
}

QTreeWidge::item:selected:!active {
    color:rgb(255,255,255);
    background: rgb(63, 200, 168);
}

QTreeWidget::branch{
    background: rgb(232, 241, 252);
}
QTreeWidget::branch:closed:has-children:!has-siblings,
QTreeWidget::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(:/image/stylesheet-branch-closed.png);
}


QTreeWidget::branch:open:has-children:!has-siblings,
QTreeWidget::branch:open:has-children:has-siblings  {
    border-image: none;
    image: url(:/image/stylesheet-branch-open.png);
}

QTreeWidget::branch:!has-children:has-siblings:adjoins-item{
    border-image:none;
    image:url(:/image/stylesheet-branch-more.png);
    }

QTreeWidget::branch:!has-children:!has-siblings:adjoins-item{
    border-image:none;
    image:url(:/image/stylesheet-branch-end.png);

}

